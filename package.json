{
  "name": "nepalify",
  "version": "0.5.0",
  "description": "A Javascript library that adds instant support for Nepali(नेपाली) input on HTML <input> and <textarea>.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "deps": "npm-check --update",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev:server": "npm run webpack-dev-server -- --env.mode development",
    "dev:build": "npm run webpack -- --env.mode development",
    "serve": "npm-run-all --parallel clean dev:server",
    "supported": "browserslist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suvash/nepalify.git"
  },
  "keywords": [
    "nepali",
    "romanized",
    "unicode",
    "keyboard",
    "layout",
    "नेपाली"
  ],
  "author": "Suvash Thapaliya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suvash/nepalify/issues"
  },
  "homepage": "https://github.com/suvash/nepalify#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.12.0",
    "core-js": "^3.6.5",
    "html-webpack-plugin": "^4.2.0",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "cover 99.5%",
    "ie 11",
    "ie 10",
    "ie 9"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          }
        }
      ]
    ]
  }
}
